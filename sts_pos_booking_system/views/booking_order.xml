<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="sts_booking_order_form_view" model="ir.ui.view">
      <field name="name">sts_booking_order_Form_view</field>
      <field name="model">booking.order</field>
      <field name="inherit_id" ref="pos_booking_system.wk_booking_order_Form_view" />
      <field name="arch" type="xml">
          <xpath expr="//field[@name='booking_date']" position="after">
              <field name="table_id" readonly="1" />
          </xpath>
      </field>
    </record>

    <record id="sts_booking_order_tree_view" model="ir.ui.view">
      <field name="name">sts_booking_order_tree_view</field>
      <field name="model">booking.order</field>
      <field name="inherit_id" ref="pos_booking_system.wk_booking_order_Tree_view" />
      <field name="arch" type="xml">
          <xpath expr="//field[@name='time_slot_id']" position="after">
              <field name="table_id" readonly="1" />
          </xpath>
      </field>
    </record>

</odoo>