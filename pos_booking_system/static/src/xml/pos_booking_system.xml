    <t t-name="WkBookingOrderLine">
        <tr class='wk-booking-line' t-att-data-id='booking.id' style="cursor:pointer;">
            <td>
                <t t-esc='booking.name' />
            </td>
            <td>
                <t t-esc="booking.customer[1] || '-'" />
            </td>
            <td>
                <t t-esc='booking.booking_date' />
            </td>
            <td>
                <t t-esc='booking.time_slot_id[1]' />
            </td>
            <td>
                <t t-esc='booking.plan_id[1]' />
            </td>
            <td>
                <t t-esc='booking.booking_product_id[1]' />
            </td>
            <td>
                <t t-esc='booking.quantity' />
            </td>
            <td>
                <t t-if="booking.booking_source == 'pos'">
                        Point Of Sale
                </t>
                <t t-else="">
                    Website
                </t>
            </td>
            <td>
                <t t-if="booking.state == 'draft'">
                        New
                </t>
                <!-- <t t-if="booking.state == 'checkin'">
                        Checked In
                </t>   -->
                <t t-if="booking.state == 'done'">
                        Done
                </t>
                <t t-if="booking.state == 'in_progress'">
                        In Progress
                </t>
                <t t-if="booking.state == 'paid'">
                        Paid
                </t>
                <t t-if="booking.state == 'cancel'">
                        Cancel
                </t>
            </td>
            <td>
                <!-- <t t-if="booking.state == 'draft'"> -->
                <!-- <t t-if="!booking.is_paid_via_pos"> -->
                <t t-if="booking.state == 'draft'">
                    <button id="process_start" t-att-booking-id="booking.id">Start</button>
                    <button id="reschedule_booking" t-att-booking-id="booking.id">Reschedule Booking</button>
                    <!-- <t t-if="booking.state == 'draft'"> -->
                    <button id="cancel" t-att-booking-id="booking.id">Cancel</button>
                </t>
                <!-- </t> -->
                <!-- </t> -->
                <!-- </t>
                <t t-else="">
                    <div style="color:red">
                        Delete
                    </div>
                </t> -->
            </td>
        </tr>
    </t>
    <t t-name="OrderSummary" t-inherit="point_of_sale.OrderSummary" t-inherit-mode="extension" owl="1">
        <xpath expr="//div[hasclass('summary')]" position='inside'>
            <div class="timer_hide" style="display:none;">
                <div id='item_count_value' style='float:left;margin-left:12px;font-weight:bold; max-width: 216px; color: #5B7641;background: rgba(110,200,155,0.17);padding: 10px;'>
                    <t t-if="env.pos.get_order() and env.pos.get_order().booking_id">
                        <div>
                            Timer:&#160;<span class="timer"><t t-esc="env.pos.get_order().remaining_time"/></span>
                        </div>
                        <div class="booking_paid" style="margin-top:13px;">
                            PAID BOOKING
                        </div>
                    </t>
                </div>
            </div>
            <!-- <br/>
            <div class="searchbox booking_paid" style="width:100%;font-size:19px;font-weight:bold;color:black;margin-top:13px;">
                PAID BOOKING
            </div> -->
        </xpath>
    </t>
    <t t-extend="Chrome" owl="1" t-inherit="point_of_sale.Chrome" t-inherit-mode="extension" >
        <xpath expr="//div[hasclass('pos-branding')]//img" position='after'>
            <ShowSlotsButtonWidget/>
        </xpath>
    </t>
    <t t-extend="PaymentScreenWidget">
        <t t-jquery=".button.next" t-operation="before">
            <span class='button done_booking'>
                <i class='fa fa-angle-double-left'></i>
                Done Booking
            </span>
        </t>
    </t>
    <t t-extend="Orderline">
        <t t-jquery="ul.info-list" t-operation="after">
            <span t-attf-class="paid_line #{line.product.id}" style="text-align:center;position: relative;bottom: 17px;font-weight: bold;float: right;background:orange;width:55px;border-radius:15px;color:white;">
                PAID
            </span>
            <t t-if="line.is_paid_line">
                <span t-attf-class="#{line.product.id}" style="text-align:center;position: relative;bottom: 17px;font-weight: bold;float: right;background:orange;width:55px;border-radius:15px;color:white;">
                    PAID
                </span>
            </t>
        </t>
        <t t-jquery="ul.info-list" t-operation="append">
            <t t-if="line.product.is_booking_type">
                <li>
                    <div class="info timer" style="font-size:20px;font-weight:bold;color:black;margin-top:5px;">
                        <div class="timer_hide" style="display:none;">
                            <t t-if="widget.pos.get_order() and widget.pos.get_order().booking_id">
                                <t t-esc="widget.pos.get_order().remaining_time"/>
                              </t>
                        </div>
                    </div>
                </li>
            </t>
        </t>
    </t>
    <t t-extend="Product">
        <t t-jquery=".product-img" t-operation="replace">
            <div class="product-img">
                <img t-att-src='image_url' />
                <t t-if="!product.to_weight">
                    <span class="price-tag">
                        <t t-if="product.is_booking_type">
                            <t t-esc="product.range_price"/>
                            <t t-if="product.range_price != 'No Slots' and product.range_price != 'No Day Slots'">
                                <t t-esc="widget.pos.currency.symbol"/>
                            </t>
                        </t>
                        <t t-else="">
                            <t t-esc="widget.format_currency(product.get_price(pricelist, 1),'Product Price')"/>
                        </t>
                    </span>
                </t>
                <t t-if="product.to_weight">
                    <span class="price-tag">
                        <t t-if="product.is_booking_type">
                            <t t-esc="product.range_price"/>
                            <t t-if="product.range_price != 'No Slots' and product.range_price != 'No Day Slots'">
                                <t t-esc="widget.pos.currency.symbol"/>
                            </t>
                        </t>
                        <t t-else="">
                            <t t-esc="widget.format_currency(product.get_price(pricelist, 1),'Product Price')+'/'+widget.pos.units_by_id[product.uom_id[0]].name"/>
                        </t>
                    </span>
                </t>
            </div>
        </t>
    </t>
</templates>
