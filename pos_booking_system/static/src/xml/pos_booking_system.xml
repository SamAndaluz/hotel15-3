    <t t-name="ShowSlotsButtonWidget" owl="1">
        <div class="slot-button" t-on-click="onClick">
            <i class="fa fa-id-card" style="margin-right:5px;" aria-hidden="true"></i>
            <div>Bookings</div>
        </div>
    </t>

