    <t t-name="ShowBookingsButtonWidget" owl="1">
        <span class="control-button" id="show_bookings" >
            Show All Bookings
        </span>
    </t>
    <t t-name="ExtendBooking" owl="1">
        <div class='control-button' id="extend_booking">
            Extend Booking
        </div>
    </t>
    <t t-name="BookingOrderScreenWidget" owl="1">
        <div class="clientlist-screen screen booking">
            <div class="orders-screen-content">
                <section class="top-content">
                    <span class='button back'>
                        <i class='fa fa-angle-double-left'></i>
                        Back
                    </span>
                </section>
                <span class="searchbox">
                    <input placeholder="Search booking" class="booking_search"/>
                    <span class="search-clear"></span>
                </span>
                <span class="sort searchbox" style="font-size:20px;font-weight:bold;left:75%;margin-top:20px;">
                    <span>Sort : </span>
                    <select  class="booking_sort" style="font-size:20px;">
                        <option value="booking_date">Booking Date</option>
                        <option value="booking_no">Booking No.</option>
                        <option value="booking_slot">Slot</option>
                    </select>
                </span>
                <!-- <span class="status searchbox" style="font-size:20px;font-weight:bold;left:75%;margin-top:20px;">
                    <span>Status : </span>
                    <select  class="booking_status" style="font-size:20px;">
                        <option value="draft">New</option>
                        <option value="cancel">Cancel.</option>
                        <option value="in_progress">In Progress</option>
                        <option value="done">Done</option>
                    </select>
                </span> -->
                <section class="full-content">
                    <div class='window'>
                        <section class='subwindow collapsed'>
                            <div class='subwindow-container collapsed'>
                                <div class='subwindow-container-fix order-details-contents'></div>
                            </div>
                        </section>
                        <section class='subwindow'>
                            <div class='wk_table_back subwindow-container'>
                                <div class='subwindow-container-fix touch-scrollable scrollable-y' style="background-color: rgb(240, 238, 238);overflow-y: scroll;">
                                    <table class='client-list wk_order_list'>
                                        <thead>
                                            <tr style="background-color: rgb(135, 160, 166);color: white;">
                                                <th>Booking No.</th>
                                                <th>Customer</th>
                                                <th>Booking Date</th>
                                                <th>Slot</th>
                                                <th>Plan</th>
                                                <th>Product</th>
                                                <th>Quantity</th>
                                                <th>Source</th>
                                                <th>Status</th>
                                                <th>Actions</th>
                                            </tr>
                                        </thead>
                                        <tbody class='wk-booking-list-contents'></tbody>
                                    </table>
                                </div>
                            </div>
                        </section>
                    </div>
                </section>
            </div>
        </div>
    </t>
