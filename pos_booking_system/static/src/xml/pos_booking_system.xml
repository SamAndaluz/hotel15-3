<templates id="template" xml:space="preserve">
    <t t-name="WkAlertPopUp" owl="1">
        <div class="modal-dialog">
            <div class="popup popup-alert">
                <p class="title" style="font-size:30px;">
                    <t t-esc=" props.title || 'Booking Notification' "/>
                </p>
                <div class="body text-center" style="height:230px;max-height:230px;overflow-y:auto;">
                    <t t-if="props.data">
                        <t t-foreach="props.data" t-as="iter">
                            <p>* The Booking
                                <t t-esc="iter.name"/> will be get finished in
                                <t t-esc="props.reminder_time"/> minutes.
                            </p>
                            <br/>
                        </t>
                    </t>
                </div>
                <div class="footer">
                    <div class="button cancel" t-on-click="cancel">
            Ok
          </div>
                </div>
            </div>
        </div>
    </t>
    <t t-name="PlanOccupied" owl="1">
        <div class="modal-dialog">
            <div class="popup popup-alert">
                <p class="title" style="font-size:30px;">
                    <t t-esc=" props.title || 'Warning!!' "/>
                </p>
                <div class="body text-center" style="height:230px;max-height:230px;overflow-y:auto;">
                    <t t-esc="props.body"/>
                </div>
                <div class="footer">
                    <div class="button cancel" t-on-click="cancel">
            Ok
          </div>
                </div>
            </div>
        </div>
    </t>
    <t t-name="WkBookReservationPopUp" owl="1">
        <div role="dialog" class="modal-dialog">
            <div class="popup popup-alert" style="max-width:80%;">
                <p class="title">
                    <button class="date_hide change_date" style="display:none;float:left">
                        <i class=" fa fa-arrow-left" style="margin-right:2px;"/>Change Date
                    </button>Make A Booking
                </p>
                <div class="body" style="height:330px;white-space:normal">
                    <div style="width:100%;display:inline-block">
                        <div class="book_booking_form check-form" style="width:70%;float:left;padding-right:2px;border-right:5px solid black;">
                            <div class="datepicker_div" style="display:inline-block;width:85%;">
                                <div id='bk_datepicker'  data-target-input="nearest" t-att-data-closing-day="props.closing_days" t-att-data-bk_end_date="props.end_date" t-att-data-bk_default_date="props.default_date" t-att-data-w_c_days="props.w_c_days" style="display:flex;">
                                    <div style="display:block;">
                                        <p style="font-size:18px;font-weight:bold;top: 8px;position: relative;font-size: 20px;color: #1CBC8C;">Booking Date:</p>
                                    </div>
                                    <div style="display:block;margin-left:20%;width:fit-content;margin-top:16px;">
                                        <input type="text" t-on-click="click_input_date" class="booking_dates" id="datepicker" placeholder="MM/DD/YYYY"/>
                                        <script>
                                    $(function() {
                                        $("#datepicker").datepicker({
                                            minDate : new Date($('#bk_datepicker').data('bk_default_date')),
                                            maxDate : new Date($('#bk_datepicker').data('bk_end_date')),
                                            beforeShowDay: function(date){
                                                var closing_days = $('#bk_datepicker').data('closing-day');
                                                var month = date.getMonth();
                                                month = parseInt(month) + 1 ;
                                                var string_to_add = '-';
                                                if(month.toString().length == 1)
                                                  string_to_add+= 0;
                                                var new_date = date.getFullYear()+string_to_add+ month+'-';
                                                if(date.getDate() &lt; 10)
                                                    new_date += '0'+date.getDate();
                                                else
                                                    new_date += date.getDate();
                                                if($('#bk_datepicker').data('w_c_days').indexOf(date.getDay()) != -1 ){
                                                    return [false, ''];
                                                }
                                                if( closing_days.includes(new_date)){
                                                    return [false, ''];
                                                }
                                                return [true, ''];
                                            }
                                        });
                                    });
                                    </script>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div style="width:28%;display:block;float:right;height:100%;">
                            <div style="font-size:20px;font-weight:bold;display:block;float:left;width:100%;margin-top:10px;">
                                <div style="color:#1CBC8C;font-size:32px;">Booking Summary:</div>
                            </div>
                            <br/>
                            <div style="float:left;width:100%;" class="booking_summary"/>
                    </div>
                    </div>
                    <div class="wk-error-text"></div>
                </div>
                <div class="footer">
                    <div class="button book" style="display:none;" t-on-click="book_reservation">
                        Book Now
                    </div>
                    <div class="button proceed-date" t-on-click="proceed_date">
                        Proceed
                    </div>
                    <div class="button cancel" t-on-click="cancel">
                        Cancel
                    </div>
                </div>
            </div>
        </div>
    </t>
    <t t-name="BookingSummary">
        <div class="summary" style="font-weight:bold;font-size:24px;overflow-y: auto;padding-right: 22px;width:100%;">
            <t t-foreach="data" t-as="slot">
                <t t-set="timeslot" t-value="data[slot]"/>
                <div style="display:inline-block;float:left;font-size:17px;width:100%;margin-top:10px;">
                    <div style="display:block;width:100%;color:black;font-size:22px;">
                        <t t-esc="timeslot.slot_name"/> :
                    </div>
                    <!-- &#160; -->
                    <div style="display:block;width:100%;color:#875A7B;margin-top:3px;">
                        <t t-set="count" t-value="0"/>
                        <t t-foreach="timeslot.plans" t-as="plan">
                            <t t-set="count" t-value="count + 1"/>
                            <t t-set="slot_plan" t-value="timeslot.plans[plan]"/>
                            <t t-esc="slot_plan.plan_name"/>
                            <t t-if="count != Object.keys(timeslot.plans).length">
                            ,
                        </t>
                        </t>
                    </div>
                </div>
            </t>
        </div>
    </t>
    <t t-name="RescheduleBookingPopUp" owl="1">
        <div role="dialog" class="modal-dialog">
            <div class="popup popup-alert" style="width:600px;height:400px;">
                <p class="title">Reschedule Booking</p>
                <div class="body">
                    <div class="book_booking_form">
                        <div class="datepicker_div" style="display:inline-block;width:85%;">
                            <div id='bk_datepickere'  data-target-input="nearest" t-att-data-bk_end_date="props.end_date" t-att-data-bk_default_date="props.default_date" t-att-data-closing-day="props.closing_days" t-att-data-w_c_days="props.w_c_days" style="display:flex;">
                                <div style="display:block;">
                                    <p style="font-size:18px;font-weight:bold;top: 8px;position: relative;font-size: 20px;color: #1CBC8C;">Booking Date:</p>
                                </div>
                                <div style="display:block;margin-left:20%;width:fit-content;margin-top:16px;">
                                    <input type="text" class="booking_date" id="datepickere" readonly="1" placeholder="MM/DD/YYYY"/>
                                    <script>
                                    $(function() {
                                        $("#datepickere").datepicker({
                                            minDate : new Date($('#bk_datepickere').data('bk_default_date')),
                                            maxDate : new Date($('#bk_datepickere').data('bk_end_date')),
                                            beforeShowDay: function(date){
                                                var closing_days = $('#bk_datepickere').data('closing-day');
                                                var month = date.getMonth();
                                                month = parseInt(month) + 1 ;
                                                var string_to_add = '-';
                                                if(month.toString().length == 1)
                                                  string_to_add+= 0;
                                                var new_date = date.getFullYear()+string_to_add+ month+'-';
                                                if(date.getDate() &lt; 10)
                                                    new_date += '0'+date.getDate();
                                                else
                                                    new_date += date.getDate();
                                                if($('#bk_datepickere').data('w_c_days').indexOf(date.getDay()) != -1 ){
                                                    return [false, ''];
                                                }
                                                if( closing_days.includes(new_date)){
                                                    return [false, ''];
                                                }
                                                return [true, ''];
                                            }
                                        });
                                    });
                                    </script>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="wk-error-text"></div>
                </div>
                <div class="footer">
                    <div class="button proceed" t-on-click="click_proceed">
                        Proceed
                    </div>
                    <div class="button cancel" t-on-click="cancel">
                        Cancel
                    </div>
                </div>
            </div>
        </div>
    </t>
    <t t-name="BookingTimeSlot">
        <div class="found_appointes">
            <div style="display:inline-block;width:100%;">
                <div style="display:inline-block;width:100%;">
                    <div class="filter-container" style="float:left;display:block;">
                        <div style="display: inline-block;width: 200px;">
                            <div style="float:left;">
                                Slot Filter:
                            </div>
                            <div>
                                <select class="filter-start">
                                    <t t-foreach="start_time_list" t-as="time">
                                        <option t-att-value="time">
                                            <t t-esc="time"/>
                                        </option>
                                    </t>
                                </select> -
                                <select class="filter-end">
                                    <t t-foreach="end_time_list" t-as="time">
                                        <option t-att-value="time">
                                            <t t-esc="time"/>
                                        </option>
                                    </t>
                                </select>
                            </div>
                        </div>
                        <button class="filter-now" t-on-click="filter_now">Filter</button>
                    </div>
                    <div class="filter-container" style="float:right;display:block;">
                        <div class="date_add">
                            <t t-esc="input_date"/>
                        </div>
                    </div>
                </div>
                <div class="row bk_border bk_modal_slots" style="margin-top:1%">
                    <t t-foreach="day_slots" t-as="d_slot">
                        <div class="timeslot" style="height:24px;" t-att-data-time_slot_id="d_slot['slot']['id']" t-att-data-start_time="d_slot['slot']['start_time']" t-att-data-end_time="d_slot['slot']['end_time']" t-att-data-slot_plans="d_slot['plans']">
                            <t t-esc="d_slot['slot']['name']"/>
                        </div>
                    </t>
                    <div t-if="!day_slots" class="col-12 text-center no_day_slots">
                        <span>No slots availble on the selected date, please choose a different date.</span>
                    </div>
                </div>
                <div class="row bk_model_plans" style="margin: 0 15px;">
                    <t t-set="d_plans" t-value="day_slots[0]['plans']"/>
                    <div class="col-12" style="padding: 1% 0 2% 0;">
                        <span class="pull-left" style="font-size: 16px;font-weight: bold;">
                            Select Plan:
                            <span class="selected_slot">
                                <t t-esc="day_slots[0]['slot']['name']"/>
                            </span>
                        </span>
                    </div>
                    <div class="row col-12 bk_plans_m_div" style="display:flex;width:100%;flex-wrap: wrap;">
                        <t t-foreach="d_plans" t-as="d_plan">
                            <t t-if="widget.pos.get_order().get_client()">
                                <t t-set="bk_av_qty" t-value="d_plan.avl_qty"/>
                                <div t-att-class='"plan_available col-md-4 col-12 " + (!(bk_av_qty > 0) ? "bk_disable" : "")' style="padding: 5px 11px;flex:1" t-att-slot="d_plan['slot']" t-att-id="d_plan['id']">
                                    <div class="max_qty_available" style="display:none;">
                                        <t t-esc="bk_av_qty"/>
                                    </div>
                                    <label style="display: block;margin-bottom: 0;">
                                        <input type="radio" name="bk_plan" required="true" t-att-id="d_plan['id']" t-att-value="d_plan['id']" style="display: none;"/>
                                        <div t-att-class='"bk_plan_div " + (!(bk_av_qty > 0) ? "bk_disable" : "")' t-att-data-plan_price="d_plan['price']">
                                            <div class="d-none d-sm-block">
                                                <div class="plan_name" style="font-size: 20px;">
                                                    <t t-esc="d_plan['name']"/>
                                                </div>
                                                <div style="font-size: 18px;font-weight: 700;">
                                                    <span class="plan_price"  t-att-plan-id="d_plan['plan_id']"  t-esc="d_plan['price']" style="white-space: nowrap;" />
                                                </div>
                                                <t t-if="bk_av_qty > 0">
                                                    <div  class="bk_plan_avl">
                                                  Available
                                              </div>
                                                </t>
                                                <t t-else="">
                                                    <div class="bk_plan_sold">
                                                Sold Out
                                              </div>
                                                </t>
                                            </div>
                                        </div>
                                    </label>
                                </div>
                            </t>
                        </t>
                    </div>
                    <div class="booking-summary">
                        <div style="display:inline-block;width:100%">
                            <div class="col-12 bk_model_cart" style="margin-top: 6px;display:block;float:right;width:100%;">
                                <div class="row" style="margin-bottom: 7px;display: block;">
                                    <div class="col-md-6 col-12 bk_total_price_div" style="width:30%;float:left;">
                                        <span>Qty: </span>
                                        <!-- <span class="bk_total_price" style="white-space: nowrap;" /> -->
                                        <select name="qty" class="book-qty">
                                            <option value="1">1</option>
                                            <t t-if="max_bk_qty.length > 1">
                                                <t t-foreach="max_bk_qty" t-as="qty">
                                                    <option t-att-value="qty">
                                                        <t t-esc="qty"/>
                                                    </option>
                                                </t>
                                            </t>
                                        </select>
                                    </div>
                                    <div class="col-md-6 col-12 bk_total_price_div" style="width:50%;float:right;">
                                        <span>Total: </span>
                                        <span class="bk_total_price" style="white-space: nowrap;" />
                                    </div>
                                </div>
                            </div>
                            <!-- <div style="font-size:20px;font-weight:bold;display:block;float:left;width:100%;margin-top:10px;">
                                <div style="color:#1CBC8C;font-size:32px;">Booking Summary:</div>
                            </div> -->
                        </div>
                        <!-- <br/>
                        <div style="float:left;width:100%;" class="booking_summary"/> -->
                    </div>
                </div>
            </div>
        </div>
    </t>
    <t t-name="RescheduleBookingTimeSlot">
        <div class="found_appointes">
            <div style="display:inline-block;width:100%;">
                <div class="row bk_border bk_modal_slots">
                    <t t-foreach="day_slots" t-as="d_slot">
                        <div class="timeslot" t-att-data-time_slot_id="d_slot['slot']['id']" t-att-data-start_time="d_slot['slot']['start_time']" t-att-data-end_time="d_slot['slot']['end_time']" t-att-data-slot_plans="d_slot['plans']">
                            <t t-esc="d_slot['slot']['name']"/>
                        </div>
                    </t>
                    <div t-if="!day_slots" class="col-12 text-center no_day_slots">
                        <span>No slots availble on the selected date, please choose a different date.</span>
                    </div>
                </div>
            </div>
        </div>
    </t>
    <t t-name="ProductItem" t-inherit="point_of_sale.ProductItem" t-inherit-mode="extension" owl="1">
        <xpath expr="//span[hasclass('price-tag')]" position="replace">
            <!-- <div class="product-name" t-attf-id="article_product_{{props.product.id}}">
